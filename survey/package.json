{
  "name": "teachinspire-survey",
  "private": true,
  "version": "1.0.0",
  "type": "module",
  "scripts": {
    "dev": "vite",
    "build": "tsc -b && vite build",
    "preview": "vite preview",
    "pages:dev": "wrangler pages dev dist --d1 DB=teachinspire-survey",
    "pages:deploy": "npm run build && wrangler pages deploy dist",
    "db:create": "wrangler d1 create teachinspire-survey",
    "db:init": "wrangler d1 execute teachinspire-survey --remote --file=./schema.sql",
    "db:init:local": "wrangler d1 execute teachinspire-survey --local --file=./schema.sql",
    "db:export": "wrangler d1 export teachinspire-survey --remote --output=./backup.sql"
  },
  "dependencies": {
    "framer-motion": "^12.23.25",
    "lucide-react": "^0.556.0",
    "react": "^19.2.0",
    "react-dom": "^19.2.0"
  },
  "devDependencies": {
    "@cloudflare/workers-types": "^4.20250109.0",
    "@types/react": "^19.2.5",
    "@types/react-dom": "^19.2.3",
    "@vitejs/plugin-react": "^5.1.1",
    "autoprefixer": "^10.4.22",
    "postcss": "^8.5.6",
    "tailwindcss": "^3.4.18",
    "typescript": "~5.9.3",
    "vite": "^7.2.4",
    "wrangler": "^4.26.0"
  }
}
